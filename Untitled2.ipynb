      "source": [
        "def square(x):\n",
        "    return x * x\n",
        "\n",
        "def increment(x):\n",
        "    return x + 1\n",
        "\n",
        "def composed_recur(y):\n",
        "    if y == 0:\n",
        "        return 1\n",
        "    return square(increment(composed_recur(y - 1)))\n",
        "\n",
        "print(composed_recur(8))"
      ],
